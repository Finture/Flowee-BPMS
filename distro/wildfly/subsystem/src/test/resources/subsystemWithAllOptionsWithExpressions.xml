<subsystem xmlns="urn:com.finture.bpm.jboss:1.1">
	<process-engines>
		<process-engine name="__test" default="${com.finture.bpm.jboss.process-engine.test.isDefault}">
			<datasource>${com.finture.bpm.jboss.process-engine.test.datasource}</datasource>
			<history-level>${com.finture.bpm.jboss.process-engine.test.history-level}</history-level>
      <configuration>${com.finture.bpm.jboss.process-engine.test.configuration}</configuration>
			<properties>
				<property name="job-acquisition-name">${com.finture.bpm.jboss.process-engine.test.property.job-acquisition-name}</property>
			</properties>
      <plugins>
        <plugin>
          <class>${com.finture.bpm.jboss.process-engine.test.plugin.ldap.class}</class>
          <properties>
            <property name="test">${com.finture.bpm.jboss.process-engine.test.plugin.ldap.property.test}</property>
            <property name="number">${com.finture.bpm.jboss.process-engine.test.plugin.ldap.property.number}</property>
            <property name="bool">${com.finture.bpm.jboss.process-engine.test.plugin.ldap.property.bool}</property>
          </properties>
        </plugin>
      </plugins>
		</process-engine>
	</process-engines>
	<job-executor>
	  <thread-pool-name>${com.finture.bpm.jboss.job-executor.thread-pool-name}</thread-pool-name>
    <core-threads>${com.finture.bpm.jboss.job-executor.core-threads}</core-threads>
    <max-threads>${com.finture.bpm.jboss.job-executor.max-threads}</max-threads>
    <queue-length>${com.finture.bpm.jboss.job-executor.queue-length}</queue-length>
    <keepalive-time>${com.finture.bpm.jboss.job-executor.keepalive-time}</keepalive-time>
    <allow-core-timeout>${com.finture.bpm.jboss.job-executor.allow-core-timeout}</allow-core-timeout>
		<job-acquisitions>
			<job-acquisition name="default">
				<acquisition-strategy>${com.finture.bpm.jboss.job-executor.job-acquisition.default.acquisition-strategy}</acquisition-strategy>                
				<properties>
					<property name="lockTimeInMillis">${com.finture.bpm.jboss.job-executor.job-acquisition.default.property.lockTimeInMillis}</property>
					<property name="waitTimeInMillis">${com.finture.bpm.jboss.job-executor.job-acquisition.default.property.waitTimeInMillis}</property>
					<property name="maxJobsPerAcquisition">${com.finture.bpm.jboss.job-executor.job-acquisition.default.property.maxJobsPerAcquisition}</property>
				</properties>
			</job-acquisition>
		</job-acquisitions>
	</job-executor>
</subsystem>