<settings xsi:schemaLocation="http://maven.apache.org/SETTINGS/1.1.0 http://maven.apache.org/xsd/settings-1.1.0.xsd"
          xmlns="http://maven.apache.org/SETTINGS/1.1.0"
          xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
    <servers>
        <server>
            <username>${env.MAVEN_NEXUS_USER}</username>
            <password>${env.MAVEN_NEXUS_PASSWORD}</password>
            <id>finture-release</id>
        </server>
        <server>
            <username>${env.MAVEN_NEXUS_USER}</username>
            <password>${env.MAVEN_NEXUS_PASSWORD}</password>
            <id>finture-snapshot</id>
        </server>
        <server>
            <username>${env.MAVEN_NEXUS_USER}</username>
            <password>${env.MAVEN_NEXUS_PASSWORD}</password>
            <id>nexus</id>
        </server>
    </servers>
    <mirrors>
        <mirror>
            <id>nexus</id>
            <mirrorOf>*</mirrorOf>
            <url>https://nexus.finture.com/repository/finture-proxy/</url>
        </mirror>
    </mirrors>
</settings>
